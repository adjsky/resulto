{
  "name": "resulto",
  "version": "0.1.0",
  "description": "TypeScript implementation of the Result type from Rust with async support",
  "scripts": {
    "ts-check": "tsc --noEmit",
    "test:watch": "vitest watch",
    "build": "pkgroll",
    "prepare": "pnpm build"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "author": "adjsky",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adjsky/resulto.git"
  },
  "keywords": [
    "result",
    "rust"
  ],
  "files": [
    "dist"
  ],
  "devDependencies": {
    "pkgroll": "^1.11.0"
  }
}
